<section id="viewAddhotel">
  <h2>Add new hotel</h2>
  <form id="formAddhotel" method="post">
    <label for="hotel">Hotel name:</label>
    <input
      type="text"
      id="hotel"
      name="name"
      placeholder="Hotel name"
      value="{{data.name}}"
    />
    <label for="city">City:</label>
    <input
      type="text"
      id="city"
      name="city"
      placeholder="City"
      value="{{data.city}}"
    />
    <label for="free-rooms">Free rooms:</label>
    <input
      type="number"
      id="free-rooms"
      name="freeRooms"
      placeholder="Free rooms"
      value="{{data.freeRooms}}"
    />
    <label for="imgUrl">Image:</label>
    <input
      type="text"
      id="imgUrl"
      name="imageUrl"
      value="{{data.imageUrl}}"
      placeholder="https://"
    />

    <input type="submit" class="create" value="Add" />
  </form>
</section>